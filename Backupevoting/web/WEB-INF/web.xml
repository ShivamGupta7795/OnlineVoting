<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>candidateone</servlet-name>
        <servlet-class>org.Voterregis</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>votercheck</servlet-name>
        <servlet-class>org.votercheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>candidateregis</servlet-name>
        <servlet-class>org.candidateregis</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>countvote</servlet-name>
        <servlet-class>org.countvote</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forpass</servlet-name>
        <servlet-class>org.forpass</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>candidateone</servlet-name>
        <url-pattern>/candidateone</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>votercheck</servlet-name>
        <url-pattern>/votercheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>candidateregis</servlet-name>
        <url-pattern>/candidateregis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>countvote</servlet-name>
        <url-pattern>/countvote</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>forpass</servlet-name>
        <url-pattern>/forpass</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>welcomeone.jsp</welcome-file>
    </welcome-file-list>
    
</web-app>
